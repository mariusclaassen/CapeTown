<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="361.0" prefWidth="476.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capetown.Controller">
   <children>
      <BorderPane prefHeight="320.0" prefWidth="389.0">
         <top>
            <HBox alignment="CENTER" prefHeight="55.0" prefWidth="456.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="stadiumButton" mnemonicParsing="false" onAction="#buttonClicked" text="Cape Town Stadium" />
                  <Button fx:id="islandButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#buttonClicked" text="Robben Island" />
                  <Button fx:id="mountainButton" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#buttonClicked" text=" Table Mountain" />
               </children>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </HBox>
         </top>
         <center>
            <ImageView fx:id="imageView" fitHeight="257.0" fitWidth="257.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <HBox prefHeight="49.0" prefWidth="389.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" prefHeight="49.0" prefWidth="476.0">
                     <children>
                        <Pane>
                           <children>
                              <Label fx:id="textLabel" contentDisplay="CENTER" onDragDetected="#buttonClicked" prefHeight="47.0" prefWidth="308.0" wrapText="true" HBox.hgrow="ALWAYS" />
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children></HBox>
         </bottom>
      </BorderPane>
   </children>
</VBox>
